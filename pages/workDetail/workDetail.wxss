.detailbox{
  margin: 30rpx;
}
.header{
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 20rpx;
}
.title{
  width: 100%;
  font-size: 18px;
  font-weight: bold ;
}
.sal{
  width: 100%;
  color: #74E8C2;
  font-size: 15px;
}
.base_info{
  font-size: 11px;
  color:#8b8b8b;
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.main_title{
  display: flex; 
  flex-wrap: nowrap;
  /* position: relative; */
}
.main_title_text{
  font-size: 19px;
  border-left: #9C7EFF solid 3px;
  padding-left: 10rpx;
  width: 30%;
  color: #00AD7B;
}
.solid{
  border-bottom:1px #ccc solid;
  align-self: center;
  width: 70%;
}
.key{
  color: #999;
  font-size: 15px;
  padding: 10rpx 0 0 40rpx ;
}
.content{
  padding: 20rpx 0;
}
.navbox{
  position: relative;
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
  margin-bottom: 120rpx;
}
.navigation{
  display: flex;
  background-color: white;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: space-between;
  position: absolute;
  width: 380rpx;
  height: 80rpx;
}
.address{
  margin-left:30rpx;
  font-size: 16px;
  color: #8b8b8b;
}
.navigation text{
  padding: 0 10rpx 0;
}
.navigation image{
  width: 60rpx;
  height: 60rpx;
}
.footer{
  position: fixed;
  bottom: 0px;
  display: flex;
  height: 110rpx;
  width: 100%;
}
.footer_left{
  background-color: white;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  width: 50%;
}
.footer_right{
  background-color: #74E8C2;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  width: 50%;
  color: white;
  padding-bottom: 10rpx;
}
.footer_right view{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20px; 
  height: 70%;
  width: 100%;
}
.footer_right span{
  height: 30%;
  font-size: 12px;
  width: 100%;
  text-align: center;
}